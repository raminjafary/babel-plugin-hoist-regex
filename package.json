{
  "name": "babel-plugin-hoist-regex",
  "version": "1.0.1",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "babel": "babel examples/index.js -o examples/index-transformed.js",
    "build-all": "babel examples -d build",
    "build-one": "babel examples/index.js -d build",
    "test": "jest -u --coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.6.2",
    "@babel/core": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "babel-plugin-tester": "9.2.0",
    "jest": "24.9.0"
  },
  "browserslist": [
    "last 2 version",
    "> 2%",
    "maintained node versions",
    "not dead"
  ],
  "jest": {
    "verbose": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "testPathIgnorePatterns": [
    "<rootDir>/src/"
  ],
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
  "collectCoverage": true
}
