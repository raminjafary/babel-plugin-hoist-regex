{
  "name": "babel-plugin-hoist-regex",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "babel": "./node_modules/.bin/babel src/script.js -o src/script-test.js",
    "build:all": "babel src -d build",
    "build:one": "babel src/script.js -d build",
    "test": "jest -u --coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "babel-plugin-tester": "^7.0.1",
    "jest": "^24.9.0"
  },
  "browserslist": [
    "last 2 version",
    "> 2%",
    "maintained node versions",
    "not dead"
  ],
  "jest": {
    "verbose": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "testPathIgnorePatterns": [
    "<rootDir>/src/"
  ],
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
  "collectCoverage": true
}
